{
  "name": "Hangout Detection",
  "version": "0.0.2",
  "manifest_version": 2,
  "description": "This extension detects the urls of tabs and sends a message the Hangout Detection Light App",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "page": "src/bg/bg.html",
    "persistent": true
  },
  "permissions": [
    "management",
    "tabs"
  ]
}